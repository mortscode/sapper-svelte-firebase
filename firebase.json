{
  "functions": {
    "predeploy": [
      "node predeploy.js"
    ],
    "source": "functions"
  },
  "firestore": {
    "rules": "config/firestore.rules",
    "indexes": "config/firestore.indexes.json"
  },
  "hosting": {
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "config/storage.rules"
  }
}
